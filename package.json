{
  "name": "dynasty-tech-portfolio",
  "version": "1.0.0",
  "description": "Dynasty Tech Solutions & Consulting Pty Ltd - Professional Portfolio Website",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && PORT=3001 npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "NODE_ENV=production node server/index.js"
  },
  "keywords": [
    "portfolio",
    "tech-consulting",
    "software-development",
    "web-development",
    "mobile-apps",
    "AI-consulting"
  ],
  "author": "Lionel Raseemela",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
